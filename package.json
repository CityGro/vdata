{
  "name": "vdeux",
  "version": "0.1.2",
  "description": "redux-vue binding",
  "main": "vdeux.common.js",
  "scripts": {
    "test": "npm run umd && BABEL_ENV=test mocha --compilers js:babel-core/register --require should test.js --recursive",
    "umd": "npm run cjs && webpack --config webpack.config.umd.js",
    "cjs": "rollup -c rollup.config.cjs.js",
    "start": "npm run umd && node lib/App.js",
    "webpack": "webpack -p",
    "watch": "webpack --watch",
    "demo": "rm -rf ./build && npm run webpack && surge -d vdeux.surge.sh -p ./build"
  },
  "repository": "gitlab.com:citygro/vdeux.git",
  "author": "Carlos Killpack <carlos@citygro.com>",
  "license": "Apache-2.0",
  "babel": {
    "env": {
      "test": {
        "presets": [
          "es2015"
        ]
      }
    }
  },
  "bugs": {
    "url": "https://gitlab.com/citygro/vdeux/issues"
  },
  "homepage": "https://gitlab.com/citygro/vdeux#readme",
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-0": "^6.1.18",
    "babel-runtime": "^6.18.0",
    "css-loader": "^0.25.0",
    "html-webpack-plugin": "^2.24.0",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "redux": "^3.0.4",
    "redux-thunk": "^2.1.0",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.3.9",
    "should": "^11.1.1",
    "style-loader": "^0.13.0",
    "vue": "^2.0.3",
    "vue-hot-reload-api": "^2.0.6",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^9.7.0",
    "vue-router": "^2.0.1",
    "webpack": "^1.12.9"
  },
  "peerDependencies": {
    "redux": "^3.0.4",
    "vue": "^2.0.3"
  }
}
