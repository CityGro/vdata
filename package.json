{
  "name": "@citygro/vdata",
  "version": "4.3.0",
  "description": "vue-js-data binding",
  "main": "vdata.common.js",
  "repository": "gitlab.com:citygro/vdata.git",
  "author": "Carlos Killpack <carlos@citygro.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://gitlab.com/citygro/vdata/issues"
  },
  "homepage": "https://gitlab.com/citygro/vdata#readme",
  "keywords": [
    "vue",
    "js-data"
  ],
  "scripts": {
    "test": "jest",
    "build": "npm run build:compile && npm run build:rollup",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "build:compile": "tsc || true",
    "build:rollup": "rollup -c rollup.conf.js",
    "pretest": "npm run lint",
    "prepare": "npm run build && npm test",
    "start": "cross-env BABEL_ENV=test node example/dev-server.js"
  },
  "jest": {
    "rootDir": "src",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.14.7",
    "@types/q": "^1.0.5",
    "cross-env": "^3.1.4",
    "css-loader": "^0.28.7",
    "jest": "*",
    "js-data-http": "3.0.0-beta.8",
    "js-data-localstorage": "3.0.0-beta.2",
    "localstorage-polyfill": "^1.0.1",
    "rollup": "^0.50.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "ts-jest": "^21.0.1",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3",
    "vue": "^2.1.4"
  },
  "peerDependencies": {
    "vue": ">=2.1.0"
  },
  "dependencies": {
    "js-data": "^3.0.1",
    "lodash-es": "^4.17.4",
    "q": "^1.4.1"
  }
}
