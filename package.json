{
  "name": "@citygro/vdata",
  "version": "5.1.2",
  "description": "vue-js-data binding",
  "main": "vdata.common.js",
  "repository": "gitlab.com:citygro/vdata.git",
  "author": "Carlos Killpack <carlos@citygro.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://gitlab.com/citygro/vdata/issues"
  },
  "homepage": "https://gitlab.com/citygro/vdata#readme",
  "keywords": [
    "vue",
    "js-data"
  ],
  "scripts": {
    "lint": "eslint src",
    "test": "jest",
    "build": "npm run build:cjs && npm run build:umd",
    "build:umd": "webpack --config webpack.conf.umd.js",
    "build:cjs": "rollup -c rollup.conf.cjs.js",
    "prepublish": "npm run build && npm test",
    "start": "cross-env BABEL_ENV=test node example/dev-server.js"
  },
  "jest": {
    "rootDir": "src",
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "jsdom"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-jest": "*",
    "babel-loader": "^6.2.0",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.18.0",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^3.1.4",
    "html-webpack-plugin": "^2.28.0",
    "jest": "*",
    "js-data-http": "3.0.0-beta.8",
    "js-data-localstorage": "3.0.0-beta.2",
    "jsdom": "^9.8.3",
    "json-server": "^0.9.5",
    "localstorage-polyfill": "^1.0.1",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "should": "^11.1.1",
    "vue": "^2.1.4",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "peerDependencies": {
    "vue": ">=2.1.0"
  },
  "dependencies": {
    "js-data": "^3.0.1",
    "lodash": "^4.17.2",
    "q": "^1.4.1"
  }
}
