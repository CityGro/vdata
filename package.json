{
  "name": "revue",
  "version": "1.2.0",
  "description": "Redux binding for Vue.",
  "main": "index.js",
  "scripts": {
    "test": "npm run babel:all && mocha --compilers js:babel-core/register --require should test.js",
    "umd": "webpack --config webpack.config.umd.js",
    "babel:all": "babel src --out-dir lib",
    "start": "npm run umd && node lib/App.js",
    "webpack": "webpack -p",
    "watch": "webpack --watch",
    "prepublish": "npm run umd && npm test",
    "demo": "npm run webpack && surge -d revue.surge.sh -p ./"
  },
  "author": "EGOIST",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "jsdom": "^7.1.0",
    "mocha": "^2.3.4",
    "redux": "^3.0.4",
    "should": "^7.1.1",
    "vue": "^1.0.10",
    "webpack": "^1.12.9"
  }
}
