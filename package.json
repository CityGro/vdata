{
  "name": "@citygro/vdata",
  "version": "1.0.1",
  "description": "vue-js-data binding",
  "main": "vdata.common.js",
  "repository": "gitlab.com:citygro/vdata.git",
  "author": "Carlos Killpack <carlos@citygro.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://gitlab.com/citygro/vdata/issues"
  },
  "homepage": "https://gitlab.com/citygro/vdata#readme",
  "keywords": [
    "vue",
    "js-data"
  ],
  "scripts": {
    "test": "jest",
    "build": "npm run build:cjs && npm run build:umd",
    "build:umd": "webpack --config webpack.config.umd.js",
    "build:cjs": "rollup -c rollup.config.cjs.js",
    "prepublish": "npm run build && npm test"
  },
  "jest": {
    "rootDir": "src",
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "jsdom"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.0",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-2": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "jest": "^17.0.3",
    "js-data-http": "^2.2.2",
    "jsdom": "^9.8.3",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.3.9",
    "should": "^11.1.1",
    "vue": "^2.1.4",
    "webpack": "^1.12.9"
  },
  "peerDependencies": {
    "js-data": "^2.10.0",
    "vue": ">=2.1.0"
  },
  "dependencies": {
    "lodash": "^4.17.2"
  }
}
